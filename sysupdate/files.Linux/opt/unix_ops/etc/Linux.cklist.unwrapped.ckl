 @@@@If a line is returned, this is a finding.</ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Fix_Text</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA>Change usernames, or delete accounts, so each has a unique name.</ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>False_Positives</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA></ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>False_Negatives</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA></ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Documentable</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA>false</ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Mitigations</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA></ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Potential_Impact</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA></ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Third_Party_Tools</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA></ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Mitigation_Control</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA></ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Responsibility</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA></ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Security_Override_Guidance</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA></ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Check_Content_Ref</VULN_ATTRIBUTE>@@@@            <ATTRIBUTE_DATA></ATTRIBUTE_DATA>@@@@        </STIG_DATA>@@@@        <STIG_DATA>@@@@            <VULN_ATTRIBUTE>Class</VULN_ATTRIBUTE>@@@